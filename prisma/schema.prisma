// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Wallets {
  wallet       String @id
  bgumMint     Int
  bgumTransfer Int
  bgumBurn     Int
  sol          Int
  tokens       Json
  tensorIX     Int
  meIX         Int
  sharkyIX     Int
  foxyIX       Int
  cmMints      Int
}
